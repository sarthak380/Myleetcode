bool canJump(int* nums, int numsSize)
{
    int jump = nums[0]; 
    
    for (int i = 1; i < numsSize; i++) 
    {
        if (jump == 0) 
        {
            return false;
        }
        
        jump--; 
        if (nums[i] > jump) 
        {
            jump = nums[i];
        }
    }
    
    return true; 
}
